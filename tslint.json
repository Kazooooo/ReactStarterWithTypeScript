{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "indent": [true, "spaces", 2],
    "ordered-imports": [false],
    "object-literal-sort-keys": false,
    "interface-name": [true, "never-prefix"],
    "no-console": [false],
    "semicolon": [true, "always", "ignore-bound-class-methods"],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ]
  },
  "rulesDirectory": []
}
